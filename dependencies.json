{
    "name" :"hf-vue-pc",
    "dependencies": {
        "javascript": {
          "@babel/polyfill": {
            "version": "7.4.3",
            "src": "node_modules/@babel/polyfill/dist/polyfill.min.js",
            "dest": "src/assets/js/polyfill.{{version}}.min.js"
          },
          "axios": {
            "version": "0.19.0",
            "src": "node_modules/axios/dist/axios.min.js",
            "dest": "src/assets/js/axios.{{version}}.min.js"
          },
          "element-ui": {
            "version": "2.12.0",
            "src": "node_modules/element-ui/lib/index.js",
            "dest": "src/assets/js/element-ui.{{version}}.min.js"
          },
          "lodash": {
            "version": "4.17.15",
            "src": "node_modules/lodash/lodash.min.js",
            "dest": "src/assets/js/lodash.{{version}}.min.js"
          },
          "vue": {
            "version": "2.6.10",
            "src": "node_modules/vue/dist/vue.min.js",
            "dest": "src/assets/js/vue.{{version}}.min.js"
          },
          "vue-router": {
            "version": "3.0.0",
            "src": "node_modules/vue-router/dist/vue-router.min.js",
            "dest": "src/assets/js/vue-router.{{version}}.min.js"
          },
          "vuex": {
            "version": "3.1.1",
            "src": "node_modules/vuex/dist/vuex.min.js",
            "dest": "src/assets/js/vuex.{{version}}.min.js"
          }
        },

        "css": {
          "element-ui": {
            "version": "2.12.0",
            "src": "node_modules/element-ui/lib/theme-chalk/index.css",
            "dest": "src/assets/style/element-ui.{{version}}/index.min.css"
          }
        },

        "font": {
          "element-ui": {
            "version": "2.12.0",
            "src": "node_modules/element-ui/lib/theme-chalk/fonts",
            "dest": "src/assets/style/element-ui.{{version}}/fonts"
          }
        }
    }
}
